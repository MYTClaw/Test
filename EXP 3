SELECT E.*, D.*
FROM EMPLO E
JOIN DEPARTMENT12 D ON E.DNO = D.DNO
WHERE D.DNAME = 'RESEARCH';

SELECT E.Fname, E.Lname, E.Address
FROM EMPLO E
JOIN DEPARTMENT12 D ON E.DNO = D.DNO
WHERE D.DNAME = 'RESEARCH';

SELECT *
FROM EMPLO
WHERE DNO = 5 AND Salary BETWEEN 30000 AND 40000;

SELECT P.PNO, P.DNO, D.MGRSSN, E.Lname AS ManagerLastName,
E.Address AS ManagerAddress, E.Bdate AS ManagerBirthdate
FROM PROJECT12 P
JOIN DEPARTMENT12 D ON P.DNO = D.DNO
JOIN EMPLO E ON D.MGRSSN = E.SSN
WHERE P.Plocation = 'BANGALORE';

SELECT DISTINCT E.Fname, E.Lname
FROM EMPLO E
LEFT JOIN DEPENDENT12 D ON E.SSN = D.ESSN
WHERE D.ESSN IS NULL;

SELECT E.SSN
FROM EMPLO E
JOIN WORKS_ON12 W ON E.SSN = W.SSN
WHERE W.Pno IN (101,102, 103);

SELECT Fname, Lname
FROM EMPLO
WHERE MSSN IS NULL;
